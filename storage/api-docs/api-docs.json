{
    "openapi": "3.0.0",
    "info": {
        "title": "Quán cà phê",
        "version": "0.1"
    },
    "paths": {
        "/api/doc": {},
        "/api/app/auth/register": {
            "post": {
                "tags": [
                    "App Tài khoản"
                ],
                "summary": "Đăng ký",
                "operationId": "app/users/create",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "email": {
                                        "type": "string"
                                    },
                                    "name": {
                                        "type": "string"
                                    },
                                    "phone_number": {
                                        "type": "string"
                                    },
                                    "username": {
                                        "type": "string"
                                    },
                                    "password": {
                                        "type": "string"
                                    }
                                },
                                "type": "object"
                            },
                            "examples": {
                                "Examples": {
                                    "summary": "Examples",
                                    "value": {
                                        "email": "user1@gmail.com",
                                        "name": "Nguyen Van A",
                                        "phone_number": "0123456789",
                                        "username": "user1",
                                        "password": "123123@"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Success."
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/app/auth/login": {
            "post": {
                "tags": [
                    "App Tài khoản"
                ],
                "summary": "Đăng nhập",
                "operationId": "app/users/login",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "username": {
                                        "type": "string"
                                    },
                                    "password": {
                                        "type": "string"
                                    },
                                    "device_token": {
                                        "type": "string"
                                    }
                                },
                                "type": "object"
                            },
                            "examples": {
                                "Examples": {
                                    "summary": "Examples",
                                    "value": {
                                        "username": "user1",
                                        "password": "123123@",
                                        "device_token": "xxx111xxx"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Success."
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "name": "bearerAuth",
                "in": "header",
                "bearerFormat": "JWT",
                "scheme": "bearer"
            }
        }
    }
}